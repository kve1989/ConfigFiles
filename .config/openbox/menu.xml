<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/3.4/menu">
	<menu id="apps-fileman-menu" label="Games">
		<item label="Gweled">
			<action name="Execute">
				<command>gweled</command>
			</action>
		</item>
	</menu>
	<menu id="apps-graphics-menu" label="Graphics">
		<item label="Gimp">
			<action name="Execute">
				<command>gimp</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<menu id="apps-graphics-menu-991824" label="Screenshot">
			<item label="Create a screenshot">
				<action name="Execute">
					<execute>scrot ~/Pictures/Screenshots/screenshot-%d-%m-%y-%T.png</execute>
				</action>
			</item>
			<item label="Create a screenshot area">
				<action name="Execute">
					<execute>scrot -s ~/Pictures/Screenshots/screenshot-%d-%m-%y-%T.png</execute>
				</action>
			</item>
		</menu>
	</menu>
	<menu id="apps-net-menu" label="Internet">
		<item label="Firefox">
			<action name="Execute">
				<command>firefox</command>
				<startupnotify>
					<enabled>yes</enabled>
					<wmclass>Firefox</wmclass>
				</startupnotify>
			</action>
		</item>
		<item label="Google Chrome">
			<action name="Execute">
				<command>google-chrome-stable</command>
			</action>
		</item>
		<menu id="apps-net-menu-275957" label="Yandex.Disk">
			<item label="Start">
				<action name="Execute">
					<execute>yandex-disk start</execute>
				</action>
			</item>
			<item label="Stop">
				<action name="Execute">
					<execute>yandex-disk stop</execute>
				</action>
			</item>
		</menu>
	</menu>
	<menu id="apps-accessories-menu" label="Tools">
		<item label="Geany">
			<action name="Execute">
				<command>geany</command>
			</action>
		</item>
		<item label="thunar">
			<action name="Execute">
				<command>thunar</command>
				<startupnotify>
					<enabled>yes</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="Urxvt">
			<action name="Execute">
				<command>urxvt</command>
			</action>
		</item>
	</menu>
	<menu id="system-menu" label="System">
		<menu id="system-menu-312255" label="Conky">
			<item label="Edit config">
				<action name="Execute">
					<execute>geany ~/.conkyrc</execute>
				</action>
			</item>
			<separator/>
			<item label="Restart conky">
				<action name="Execute">
					<execute>conkywonky</execute>
				</action>
			</item>
		</menu>
		<menu id="system-menu-903541" label="Openbox">
			<item label="GUI Config Tool">
				<action name="Execute">
					<execute>obconf</execute>
				</action>
			</item>
			<item label="GUI Menu Editor">
				<action name="Execute">
					<execute>obmenu</execute>
				</action>
			</item>
			<separator/>
			<item label="Edit autostart">
				<action name="Execute">
					<execute>geany ~/.config/openbox/autostart</execute>
				</action>
			</item>
			<item label="Edit menu.xml">
				<action name="Execute">
					<execute>geany ~/.config/openbox/menu.xml</execute>
				</action>
			</item>
			<item label="Edit rc.xml">
				<action name="Execute">
					<execute>geany ~/.config/openbox/rc.xml</execute>
				</action>
			</item>
		</menu>
		<menu id="system-menu-65138" label="Notifications">
			<item label="Setting">
				<action name="Execute">
					<execute>xfce4-notifyd-config</execute>
				</action>
			</item>
		</menu>
		<menu id="system-menu-523756" label="tint2">
			<item label="Edit config">
				<action name="Execute">
					<execute>geany ~/.config/tint2/tint2rc</execute>
				</action>
			</item>
			<separator/>
			<item label="Restart tint2">
				<action name="Execute">
					<execute>tint2restart</execute>
				</action>
			</item>
		</menu>
		<item label="Customize appearance">
			<action name="Execute">
				<command>lxappearance</command>
			</action>
		</item>
		<item label="Choose Wallpaper">
			<action name="Execute">
				<execute>nitrogen</execute>
			</action>
		</item>
		<item label="XScreenSaver">
			<action name="Execute">
				<command>xscreensaver-demo</command>
			</action>
		</item>
		<separator/>
		<item label="Update OpenBox">
			<action name="Reconfigure"/>
		</item>
	</menu>
	<menu id="root-menu" label="Openbox 3">
		<item label="Firefox">
			<action name="Execute">
				<command>firefox</command>
			</action>
		</item>
		<item label="Google Chrome">
			<action name="Execute">
				<command>google-chrome-stable</command>
			</action>
		</item>
		<item label="File Manager">
			<action name="Execute">
				<command>thunar</command>
			</action>
		</item>
		<item label="Midnight Commander">
			<action name="Execute">
				<command>urxvt -geometry 100x50 -e mc</command>
			</action>
		</item>
		<item label="Terminal">
			<action name="Execute">
				<command>urxvt</command>
			</action>
		</item>
		<separator/>
		<item label="Video">
			<action name="Execute">
				<command>thunar ~/Videos</command>
			</action>
		</item>
		<item label="Downloads">
			<action name="Execute">
				<command>thunar ~/Downloads</command>
			</action>
		</item>
		<item label="Pictures">
			<action name="Execute">
				<command>thunar ~/Pictures</command>
			</action>
		</item>
		<item label="Yandex.Disk">
			<action name="Execute">
				<command>thunar ~/Yandex.Disk</command>
			</action>
		</item>
		<separator/>
		<menu id="apps-editors-menu"/>
		<menu id="apps-graphics-menu"/>
		<menu id="apps-fileman-menu"/>
		<menu id="apps-net-menu"/>
		<menu id="apps-accessories-menu"/>
		<menu id="apps-office-menu"/>
		<menu id="apps-multimedia-menu"/>
		<menu id="apps-term-menu"/>
		<separator/>
		<menu id="system-menu"/>
		<separator/>
		<item label="Log Out">
			<action name="Exit">
    </action>
		</item>
	</menu>
</openbox_menu>
