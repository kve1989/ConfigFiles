dunst -config .config/dunst/dunstrc &
nitrogen --restore &
tint2 &
thunar --daemon &
setxkbmap -layout us,ru -option "grp:ctrl_shift_toggle" &
cbatticon &
(sleep 5 && nm-applet) &
(sleep 10 && volumeicon) &
(sleep 15 && fbxkb) &
(sleep 20 && yandex-disk-indicator) &
